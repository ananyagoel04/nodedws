            <!-- Modal Backdrop with Blur and Transition -->
            <div id="modal1"
                class="modal fixed inset-0 bg-opacity-50 w-full h-full flex justify-center z-50 items-center scale-100 opacity-100 transition-all duration-500 ease-out">
                <div class="absolute top-0 p-2 z-50 bg-opacity-50 w-full">
                    <h2 class="text-xl text-whit absolute left-0 px-4 py-4 font-semibold text-center w-full "
                        id="imageSlideshowTitle">
                    </h2>
                </div>
                <div
                    class="modal-content px-4 py-10 pt-12 shadow-md flex flex-col items-center relative bg-black bg-opacity-50 w-full h-full overflow-hidden">
                    <div class="relative w-full h-full flex-grow overflow-hidden">
                        <!-- Ad Image -->
                        <img src="data:image/png;base64,<%= randomAd.image.toString('base64') %>" loading="lazy"
                            alt="<%= randomAd.title %>" class="w-full h-full object-contain rounded-md -top-2">
                    </div>
                </div>

                <div class="absolute bottom-0 p-4 w-full">
                    <p class="text-2xl font-medium text-white text-center">
                    </p>
                </div>
            </div>

            <script>
                // Close modal function
                function closeModal(modalId) {
                    const modal = document.getElementById(modalId);
                    modal.classList.add('hidden');
                    enableScrolling();
                }
                function disableScrolling() {
                    document.body.style.overflow = 'hidden';
                }
                function enableScrolling() {
                    document.body.style.overflow = '';
                }
                document.addEventListener('DOMContentLoaded', function () {
                    disableScrolling();

                    // Automatically close the modal after 10 seconds
                    setTimeout(function () {
                        closeModal('modal1');
                    }, 10000); // Close modal after 10 seconds (10000 ms)
                });
            </script>

            <style>
                .modal {
                    transition: opacity 0.5s ease-out, transform 0.5s ease-out;
                    transform: scale(0.9);
                    opacity: 0;
                }

                .modal.scale-100 {
                    transform: scale(1);
                    opacity: 1;
                }
            </style>
            <% } %>

                <% if (!cookieConsent) { %>
                    <div id="cookieConsent"
                        class="fixed bottom-0 left-0 right-0 bg-black bg-opacity-75 text-white p-2 text-center z-50">
                        <p class="text-sm">This website uses cookies to improve your experience. By continuing to
                            browse, you agree to our cookie policy.</p>
                    </div>

                    <script>
                        // Cookie consent fade out logic
                        document.addEventListener('DOMContentLoaded', function () {
                            setTimeout(function () {
                                var cookieConsentElement = document.getElementById('cookieConsent');
                                if (cookieConsentElement) {
                                    cookieConsentElement.style.transition = 'opacity 0.5s ease-out';
                                    cookieConsentElement.style.opacity = 0;

                                    setTimeout(function () {
                                        cookieConsentElement.style.display = 'none';
                                    }, 500);
                                }
                            }, 5000);
                        });
                    </script>
                    <% } %>
