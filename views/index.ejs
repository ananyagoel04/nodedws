            <script>
                // Function to update data-aos based on screen width
                function updateAOSBasedOnScreenSize() {
                    // Select the specific <ul> by its ID and get all <li> elements inside it
                    const listItems = document.querySelectorAll("#myList li");

                    // Get the current screen width
                    const screenWidth = window.innerWidth;

                    // Loop through each list item and update the data-aos attribute
                    listItems.forEach((item, index) => {
                        let newAosValue = '';

                        if (screenWidth >= 1024) {  // For desktop (Large screen)
                            // Desktop view, use different data-aos for larger screens
                            switch (index) {
                                case 0:
                                    newAosValue = 'fade-up-right';
                                    break;
                                case 1:
                                    newAosValue = 'fade-up-right';
                                    break;
                                case 2:
                                    newAosValue = 'fade-up-left';
                                    break;
                                case 3:
                                    newAosValue = 'fade-up-left';
                                    break;
                                case 4:
                                    newAosValue = 'fade-up-right';
                                    break;
                                case 5:
                                    newAosValue = 'fade-up-left';
                                    break;
                                case 6:
                                    newAosValue = 'fade-up-left';
                                    break;
                            }
                        } else if (screenWidth >= 768) {  // For tablets (Medium screen)
                            // Tablet view, adjust delay or use different animation
                            switch (index) {
                                case 0:
                                    newAosValue = 'fade-up';
                                    break;
                                case 1:
                                    newAosValue = 'fade-up';
                                    break;
                                case 2:
                                    newAosValue = 'fade-up';
                                    break;
                                case 3:
                                    newAosValue = 'fade-up';
                                    break;
                                case 4:
                                    newAosValue = 'fade-up';
                                    break;
                                case 5:
                                    newAosValue = 'fade-up';
                                    break;
                                case 6:
                                    newAosValue = 'fade-up';
                                    break;
                            }
                        } else {  // For mobile (Small screen)
                            // Mobile view, adjust delay or use different animation
                            switch (index) {
                                case 0:
                                    newAosValue = 'fade-up';
                                    break;
                                case 1:
                                    newAosValue = 'fade-up';
                                    break;
                                case 2:
                                    newAosValue = 'fade-up';
                                    break;
                                case 3:
                                    newAosValue = 'fade-up';
                                    break;
                                case 4:
                                    newAosValue = 'fade-up';
                                    break;
                                case 5:
                                    newAosValue = 'fade-up';
                                    break;
                                case 6:
                                    newAosValue = 'fade-up';
                                    break;
                            }
                        }

                        // Update the data-aos attribute for the list item
                        item.setAttribute('data-aos', newAosValue);
                    });
                }

                // Initial function call to update AOS on page load
                updateAOSBasedOnScreenSize();

                // Add an event listener to update data-aos when the window is resized
                window.addEventListener('resize', updateAOSBasedOnScreenSize);
            </script>

